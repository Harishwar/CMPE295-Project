<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context="cmpe.alpha.fitwhiz.controllers.SensorCurrentFragment"
android:background="@drawable/background">
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/fragment_sensor_current"
        android:stretchColumns="*">

        <View style="@style/Divider"/>

        <!--accelerometer-->
        <TableRow android:layout_width="0dip" android:background="@color/black_overlay"
            android:layout_weight="2" android:layout_height="fill_parent">
            <TextView
                android:gravity="left"
                android:background="@color/black_overlay"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/sensor_acc_text"
                android:id="@+id/sensor_acc_text_live"
                android:textColor="#ffffff"
                android:layout_marginEnd="5dp"
                android:textSize="@dimen/sensor_title_dimen"/>
            <TextView
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/title_x"
                android:id="@+id/sensor_acc_text_live_x"
                android:layout_column="1"
                android:textColor="#ffffff"/>
            <TextView
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/default_string_result"
                android:layout_column="2"
                android:id="@+id/accelerometer_current_val_x"
                android:textColor="#ffffff"/>
            <TextView
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/accelerometer_units"
                android:layout_column="3"
                android:id="@+id/accelerometer_units_x"/>
        </TableRow>
        <TableRow android:layout_width="0dip" android:background="@color/black_overlay"
            android:layout_weight="2" android:layout_height="fill_parent">
            <TextView
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/title_y"
                android:id="@+id/sensor_acc_text_live_y"
                android:layout_column="1"
                android:textColor="#ffffff"/>
