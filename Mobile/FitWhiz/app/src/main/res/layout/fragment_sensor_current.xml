                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/accelerometer_units"
                android:layout_column="3"
                android:id="@+id/accelerometer_units_y"/>
        </TableRow>
        <TableRow android:layout_width="0dip" android:background="@color/black_overlay"
            android:layout_weight="2" android:layout_height="fill_parent">
