        <TableRow android:layout_width="0dip" android:background="@color/black_overlay"
            android:layout_weight="2" android:layout_height="fill_parent">
            <TextView
                android:gravity="left"
                android:background="@color/black_overlay"
                android:layout_marginEnd="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/sensor_lumen_text"
                android:id="@+id/sensor_lumen_text_live"
                android:textColor="#ffffff"
                android:textSize="@dimen/sensor_title_dimen"/>
            <TextView
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/default_string_result"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="#ffffff"
                android:layout_column="2"
                android:id="@+id/lumen_current"/>
            <TextView
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/lumen_units"
                android:layout_column="3"
                android:id="@+id/lumen_units"/>
        </TableRow>

        <View style="@style/Divider"/>

        <!-- Steps -->
        <TableRow >
            <TextView
                android:gravity="left"
                android:background="@color/black_overlay"
                android:layout_marginEnd="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#ffffff"
                android:textSize="@dimen/sensor_title_dimen"
                android:text="Step Count"
                android:id="@+id/step_count_label" />
            <TextView
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:text="0"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="#ffffff"
                android:id="@+id/stepCountValue"/>
            <TextView
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="3"
                android:text="@string/steps_units"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="#ffffff"
                android:id="@+id/stepCountUnits"/>
        </TableRow>

        <View style="@style/Divider"/>

    </TableLayout>
</ScrollView>
